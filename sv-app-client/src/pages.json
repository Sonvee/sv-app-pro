{
  "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/hall/hall",
      "style": {
        "navigationBarTitleText": "大厅",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/home/home",
      "style": {
        "navigationBarTitleText": "主页",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/mine/mine",
      "style": {
        "navigationBarTitleText": "我的",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/login/login",
      "style": {
        "navigationBarTitleText": "登录",
        "enablePullDownRefresh": false
      }
    }
  ],
  "subPackages": [{
      "root": "pages/setting",
      "pages": [{
        "path": "setting",
        "style": {
          "navigationBarTitleText": "设置"
        }
      }]
    },
    {
      "root": "pages/agreements",
      "pages": [{
          "path": "privacy",
          "style": {
            "navigationBarTitleText": "隐私政策条款"
          }
        },
        {
          "path": "service",
          "style": {
            "navigationBarTitleText": "用户服务协议"
          }
        }
      ]
    },
    {
      "root": "pages/usercenter",
      "pages": [{
          "path": "usercenter",
          "style": {
            "navigationBarTitleText": "个人中心"
          }
        },
        {
          "path": "userinfo",
          "style": {
            "navigationBarTitleText": "个人资料"
          }
        }
      ]
    },
    {
      "root": "pages/auxiliary",
      "pages": [{
        "path": "crop-image/crop-image",
        "style": {
          "navigationBarTitleText": "图片裁剪"
        }
      }]
    }
  ],
  "globalStyle": {
    "navigationBarTitleText": "sv-app-pro",
    "navigationBarBackgroundColor": "#ffffff",
    "navigationBarTextStyle": "white",
    "navigationStyle": "custom",
    "backgroundColor": "#f8f8f8",
    "app-plus": {
      "titleNView": false,
      "background": "#22273D",
      "bounce": "none"
    }
  },
  "uniIdRouter": {},
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uv-(.*)": "@climblee/uv-ui/components/uv-$1/uv-$1.vue"
    }
  },
  "tabBar": {
    "height": "0.000001px",
    "list": [{
      "pagePath": "pages/index/index"
    }, {
      "pagePath": "pages/hall/hall"
    }, {
      "pagePath": "pages/home/home"
    }, {
      "pagePath": "pages/mine/mine"
    }]
  }
}